      {
        "id": 8,
        "title": "User Activity",
        "type": "graph",
        "targets": [
          {
            "expr": "active_users_total",
            "refId": "A",
            "legendFormat": "{{time_window}}"
          }
        ],
        "yAxes": [
          {
            "label": "Active Users",
            "show": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 16
        }
      }
    ],
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": [
        {
          "name": "Revenue Milestones",
