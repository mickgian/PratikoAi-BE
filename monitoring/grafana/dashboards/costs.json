{
  "id": null,
  "title": "PratikoAI - Cost Management",
    "tags": ["pratikoai", "costs", "financial"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Cost per User vs Target (€2.00)",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(user_monthly_cost_eur)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyEUR",
            "decimals": 2,
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 1.5
                },
                {
                  "color": "orange",
                  "value": 2.0
                },
                {
                  "color": "red",
                  "value": 2.5
                }
              ]
            }
          }
        },
        "options": {
          "text": {
            "titleSize": 16,
            "valueSize": 32
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Total LLM Costs (Last 24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "increase(llm_cost_total_eur[24h])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyEUR",
            "decimals": 2,
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 50
                },
                {
                  "color": "red",
                  "value": 100
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Cost Efficiency Target",
        "type": "gauge",
        "targets": [
          {
            "expr": "(2.0 - avg(user_monthly_cost_eur)) / 2.0 * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 50
                },
                {
                  "color": "green",
                  "value": 75
                }
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value"
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "LLM Costs by Provider",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (provider) (increase(llm_cost_total_eur[24h]))",
            "refId": "A",
            "legendFormat": "{{provider}}"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "pieType": "pie",
          "tooltip": {
            "mode": "single",
            "sort": "none"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Cost Trends Over Time",
        "type": "graph",
        "targets": [
          {
            "expr": "avg(user_monthly_cost_eur)",
            "refId": "A",
            "legendFormat": "Avg Cost per User"
          },
          {
            "expr": "2.0",
            "refId": "B",
            "legendFormat": "Target (€2.00)"
          }
        ],
        "yAxes": [
          {
            "label": "Cost (EUR)",
            "show": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "API Calls by Provider",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(api_calls_total[5m])",
            "refId": "A",
            "legendFormat": "{{provider}} - {{status}}"
          }
        ],
        "yAxes": [
          {
            "label": "Calls/sec",
            "show": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 7,
        "title": "Cost per User Distribution",
        "type": "histogram",
        "targets": [
          {
            "expr": "user_monthly_cost_eur",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      }
    ],
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": [
        {
          "name": "provider",
          "type": "query",
          "query": "label_values(llm_cost_total_eur, provider)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Cost Target Breach",
          "enable": true,
          "iconColor": "red",
          "query": "ALERTS{alertname=\"HighUserCost\"}"
        }
      ]
    },
    "refresh": "1m",
    "schemaVersion": 27,
    "version": 1,
    "links": []
}